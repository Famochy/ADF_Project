{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ade4datafact"
		},
		"AzureSqlDatabase_linkservice_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase_linkservice'"
		},
		"AzureKeyVault_linkservice_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://My-Key-Vault4Use.vault.azure.net/"
		},
		"AzureBlobStorage_linkservice_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "blobkeyvault"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ade4datafact_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabase_linkservice_connectionString": {
						"value": "[parameters('AzureSqlDatabase_linkservice_connectionString')]"
					},
					"AzureKeyVault_linkservice_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault_linkservice_properties_typeProperties_baseUrl')]"
					},
					"AzureBlobStorage_linkservice_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('AzureBlobStorage_linkservice_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}